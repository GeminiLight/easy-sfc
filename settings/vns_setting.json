{
    "num_vns": 2000,
    "topology": {
        "type": "random",
        "random_prob": 0.5
    },
    "vn_size": {
        "distribution": "uniform", 
        "dtype": "int",
        "low": 2,
        "high": 10
    },
    "lifetime": {
        "distribution": "exponential", 
        "dtype": "float",
        "scale": 500
    },
    "arrival_rate": {
        "distribution": "possion", 
        "dtype": "float",
        "lam": 0.1,
        "reciprocal": true
    },
    "node_attrs_setting": [
        {
            "name": "cpu", 
            "owner": "node", 
            "type": "resource",
            "generative": true, 
            "dtype": "int",
            "distribution": "uniform", 
            "high": 50,
            "low": 0
        }
    ],
    "edge_attrs_setting": [
        {
            "name": "bw",
            "owner": "edge", 
            "type": "resource",
            "generative": true, 
            "dtype": "int",
            "distribution": "uniform",
            "high": 50,
            "low": 0
        }
    ],
    "save_dir": "dataset/vns",
    "vns_save_dir": "vns",
    "vns_file_name": "vn.gml",
    "event_file_name": "event.json",
    "setting_file_name": "vns_setting.json"
}